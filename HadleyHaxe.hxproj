  <!-- Build options -->
  <build>
    <option directives="flixel=4.5.0&#xA;openfl=8.4.1&#xA;lime=7.0.0&#xA;hscript=2.2.0&#xA;FLX_RECORD&#xA;tools=7.0.0&#xA;lime-canvas&#xA;howlerjs&#xA;no-compilation&#xA;openfl-html5&#xA;lime-webgl&#xA;lime-dom&#xA;lime-howlerjs&#xA;lime-html5&#xA;html5&#xA;web&#xA;html5&#xA;display" />
